{
    "name": "auto-form",
    "description": "auto-form extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:3000/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16.png",
            "24": "images/24.png",
            "32": "images/32.png",
            "48": "images/48.png",
            "64": "images/64.png"
        },
        "default_title": "Auto Form"
    }
}